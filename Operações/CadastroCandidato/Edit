// Edit CANDIDATO
app.put('/candidato/edit', function (req, res) {
    const { idCliente, nmCliente, dsEmail, nrTelefone } = req.body;

    conn.query('UPDATE tbCandidato set nmCliente = ?, dsEmail = ?, nrTelefone = ? WHERE idCliente = ?',
        [nmCliente, dsEmail, nrTelefone, idCliente], function (error, results) {
            if (error) throw error;
            res.status(200).json(results)
        })

});