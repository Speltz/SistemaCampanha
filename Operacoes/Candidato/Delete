// Delete CANDIDATO
app.delete('/candidato/delete', function (req, res) {
    const { nrCandidato } = req.body;

    conn.query('DELETE FROM tbCandidato WHERE nrCandidato = ?',
        [nrCandidato], function (error, results) {
            if (error) throw error;
            res.status(200).json(results)
        })

});